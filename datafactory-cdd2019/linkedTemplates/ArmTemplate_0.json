			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/AwCustomersTable')]",
				"[concat(variables('factoryId'), '/datasets/CustomersTarget')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/Once Per minute')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "Copy Customers Pipeline",
							"type": "PipelineReference"
						},
						"parameters": {}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 1,
						"startTime": "2019-05-28T21:02:00.000Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/Copy Customers Pipeline')]"
